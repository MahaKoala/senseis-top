{
  "scripts": {
    "start": "nf start",
    "clean": "rm -rf ./dist && mkdir ./dist",
    "build": "yarn run clean && yarn run build-server && yarn run build-client",
    "stylelint": "stylelint **/*.scss --fix",
    "start-server": "nodemon --watch ./src/server --watch ./src/shared --exec \" node src/server/\"",
    "start-client": "webpack-dev-server --config webpack.dev.js",
    "build-server": "yarn run babel-server && yarn run babel-shared",
    "build-client": "BABEL_ENV=production webpack -p --config webpack.prod.js && rsync -r ./static/ ./dist/static/",
    "babel-server": "BABEL_ENV=production babel ./src/server --out-dir ./dist/server",
    "babel-shared": "BABEL_ENV=production babel ./src/shared --out-dir ./dist/shared",
    "prettier": "prettier --write **/*.{js,jsx,json,scss}",
    "eslint": "eslint **/*.{js,jsx} --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "name": "senseis-top",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Alex Zaworski",
  "lint-staged": {
    "*.scss": [
      "stylelint --fix",
      "git add"
    ],
    "*.{js,jsx,scss,json}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.11",
    "dotenv-webpack": "^1.5.5",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "foreman": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.8",
    "lint-staged": "^7.1.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.17.2",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.13.4",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.1.0",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "webpack": "^4.11.0",
    "webpack-cli": "^3.0.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "nosleep.js": "^0.7.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-hot-loader": "^4.0.0",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "ws": "^5.1.0"
  }
}
